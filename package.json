{
    "name": "curry-syntax-highlighting",
    "displayName": "Curry Syntax Highlighting",
    "description": "For a more colorful Curry.",
    "version": "0.0.8",
    "publisher": "jpsikorra",
    "license": "MIT",
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/jpsikorra/vscode-curry-syntax-highlight"
    },
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "curry",
                "aliases": [
                    "Curry",
                    "curry"
                ],
                "extensions": [
                    ".curry",
                    ".fcy"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "curry",
                "scopeName": "source.curry",
                "path": "./syntaxes/curry.tmLanguage"
            }
        ]
    }
}
